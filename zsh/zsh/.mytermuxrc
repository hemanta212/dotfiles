persync(){
  cd /sdcard/.cache/personal
  git checkout org
  git pull origin org
  git add .
  git commit -a -m "Updates org file"
  git push origin org
}

peredit(){
  current = $pwd
  cd /sdcard/.cache/personal/org
  git checkout org
  emacs track.org
  cd $current
}

e(){
  emacsclient -t -s $PREFIX/var/run/emacs$(id -u)/server $@
}
