peredit(){
  cd ~/dev/personal/org
  git checkout org
  emacs track.org
}

persync(){
  cd ~/dev/personal/org
  git checkout org
  git commit -a -m "Updates org file"
  git pull origin org
  git push origin org
}

manim() {
  poetry run manim $1.py $2 --low_quality --video_output_dir=./videos
  curl localhost:60262/manim
}
